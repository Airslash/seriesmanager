--------------------------------------------------
Architecture de la base de donnée :
--------------------------------------------------

series :
	id           = int(10) UNSIGNED NOT NULL, -- PRIMARY KEY
	imdb_id      = varchar(191), -- INDEX -- UNIQUE
	title        = varchar(191), -- INDEX
	poster_id    = varchar(255),
	summary      = varchar(255),
	actors       = varchar(255),
	genre        = varchar(255),
	season_count = int(10) UNSIGNED NOT NULL,
	start_date   = year(4) NOT NULL,
	end_date     = year(4) DEFAULT NULL

episodes :
	id        = int(10) UNSIGNED NOT NULL, -- PRIMARY KEY
	imdb_id   = varchar(191), -- INDEX -- UNIQUE
	serie_id  = int(10) UNSIGNED NOT NULL, -- INDEX
	title     = varchar(191), -- INDEX
	poster_id = varchar(255),
	summary   = varchar(255),
	season    = int(10) UNSIGNED NOT NULL,
	episode   = int(10) UNSIGNED NOT NULL,
	air_date  = varchar(255)

users :
	id       = int(10) UNSIGNED NOT NULL, -- PRIMARY KEY
	email    = varchar(191), -- INDEX -- UNIQUE
	password = varchar(255),
	token    = varchar(255),
	username = varchar(191) -- UNIQUE

@todo  Demander à Guillaume si c'est nécessaire de mettre des index sur ces tables
